<!DOCTYPE html>
<html>
<head>
  <title>Testapp</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= javascript_include_tag :defaults, "rails.validations", "jquery_ujs" %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="user_nav">
  <% if current_user %>
    Welcome, <%= current_user.name %>!
    (<%= link_to "Sign Out", signout_path %>)
    <br />[<%= link_to 'Articles', articles_path %>] [<%= link_to 'Users', users_path %>]<br /><br />
  <% else %>
    <%= link_to "Sign in with Twitter", "/auth/twitter" %>
  <% end %>
</div>



<%= raw flash_messages %>
<%= yield %>

</body>
</html>
